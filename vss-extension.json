{
    "manifestVersion": 1,
    "id": "PostToOffice365Connector",
    "name": "Post To Office 365 Connector",
    "version": "0.1.4",
    "publisher": "freerangeeggs",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "Send a build or release message to an Office365 connector, allowing you to notify users in Microsoft Teams and Office365 Groups.",
    "icons": {
