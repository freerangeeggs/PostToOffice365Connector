        "Public"
    ],
    "categories": [
        "Build and release"
    ],
    "tags": [
    ],
        "screenshots": [            
        {
            "path": "images/screenshot3.png"
        },
        {
            "path": "images/screenshot2.png"
        },
        {
            "path": "images/screenshot1.png"
        }
    ],
    "files": [
        {
            "path": "PostToOffice365Connector/node_modules"
        },
        {
            "path": "PostToOffice365Connector/task.json"
        },
        {
            "path": "PostToOffice365Connector/task.js"
        },
        {
            "path": "PostToOffice365Connector/sendpackage.js"
        },
        {
            "path": "PostToOffice365Connector/icon.png"
        }
    ],
    "content" : {
        "details": {
            "path": "overview.md"
        }
    },
